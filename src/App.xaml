<Application x:Class="Desktop_Frontend.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Desktop_Frontend"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Color x:Key="PrimaryColor">#1B262C</Color>
        <Color x:Key="SecondaryColor">#0F4C75</Color>
        <Color x:Key="TertiaryColor">#FFFFFF</Color>
        <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}" />
        <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource SecondaryColor}" />
        <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource TertiaryColor}" />
    </Application.Resources>
</Application>
